{
  "name": "gravity-ui-vite-example",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "react",
    "vite",
    "template",
    "typescript",
    "gravity",
    "uikit",
    "sass"
  ],
  "description": "Vite template with: typescript, sass and @gravity-ui packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/gravity-ui/gravity-ui-vite-example"
  },
  "engines": {
    "node": ">=18"
  },
  "bugs": {
    "url": "https://github.com/gravity-ui/gravity-ui-vite-example/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview --port 3737",
    "preview": "vite preview",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier",
    "test": "exit 0"
  },
  "dependencies": {
    "@gravity-ui/icons": "^2.6.0",
    "@gravity-ui/uikit": "^5.31.2",
    "@react-google-maps/api": "^2.19.3",
    "@reduxjs/toolkit": "^2.2.5",
    "antd": "^5.23.3",
    "axios": "^1.7.2",
    "bem-cn-lite": "^4.1.0",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-select": "^5.8.0",
    "tesseract.js": "^5.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.1.0",
    "@gravity-ui/chartkit": "^5.10.2",
    "@gravity-ui/eslint-config": "^3.1.1",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@types/antd": "^0.12.32",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^22.13.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "esbuild-sass-plugin": "^2.16.1",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "sass": "^1.68.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
